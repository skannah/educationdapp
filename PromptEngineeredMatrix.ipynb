    "import numpy as np\n",
    "\n",
    "# Define matrices X and W\n",
    "X = np.array([[1, 2], [-1, -2], [0.1, 0.2]])\n",
    "W = np.array([[3, 4, 5, 6], [7, 8, 9, 0]])\n",
    "\n",
    "# Perform matrix multiplication\n",
    "result = np.matmul(X, W)\n",
    "\n",
    "print(\"Result of matrix multiplication:\")\n",
    "print(result)\n"
